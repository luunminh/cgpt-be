    "@nestjs/cqrs": "10.2.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "10.4.5",
    "@nestjs/swagger": "^7.4.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cache-manager": "4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.4.3",
    "helmet": "^8.0.0",
    "joi": "17.13.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "nestjs-pino": "4.1.0",
    "passport-jwt": "^4.0.1",
    "pino-http": "10.3.0",
