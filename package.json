  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --debug --watch",
    "start:dev": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "detect-secrets": "git diff --staged --name-only | xargs detect-secrets-hook --baseline .secrets.baseline --exclude-files '.*.npmrc$'",
    "detect-secrets:scan": "detect-secrets scan --baseline .secrets.baseline",
    "detect-secrets:audit": "detect-secrets audit .secrets.baseline",
    "prisma:m": "prisma migrate dev",
    "prisma:m-c": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:g": "prisma generate",
    "prisma:seed": "prisma db seed",
    "fix:husky": "chmod +x .husky/pre-commit && chmod +x bin/detect-secrets",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "prisma": {
    "schema": "./src/system/database/prisma/schema.prisma",
    "seed": "ts-node ./src/system/database/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.3",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.4.5",
    "@nestjs/config": "3.2.3",
