    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-security": "2.1.1",
    "husky": "9.0.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prisma": "5.16.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-essentials": "^10.0.2",
